{
  "name": "exo",
  "packageManager": "yarn@3.2.0",
  "private": true,
  "scripts": {
    "cy:open": "cypress open",
    "cy:run": "cross-env CYPRESS_baseUrl=http://localhost:9000 cypress run",
    "format": "prettier \"**/*.{ts,js,tsx,jsx,md,mdx,json,yaml}\" --write",
    "lint": "eslint . --ignore-path .gitignore --ignore-path .prettierignore --ext .ts,.tsx,.js -f mo",
    "lint:fix": "yarn lint --fix",
    "test": "yarn jest",
    "test:watch": "yarn jest --watch",
    "tsc": "tsc",
    "type-check": "tsc --noEmit"
  },
  "devDependencies": {
    "@testing-library/cypress": "^8.0.2",
    "@testing-library/jest-dom": "^5.16.4",
    "@testing-library/react": "^13.0.1",
    "@types/jest": "^27.4.1",
    "@types/node": "^17.0.23",
    "@types/react": "^18.0.4",
    "@types/react-dom": "^18.0.0",
    "@typescript-eslint/eslint-plugin": "^5.19.0",
    "@typescript-eslint/parser": "^5.19.0",
    "cross-env": "^7.0.3",
    "cypress": "^9.5.4",
    "eslint": "^8.13.0",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-config-prettier": "^8.5.0",
    "eslint-formatter-git-log": "^0.5.3",
    "eslint-formatter-mo": "^1.1.1",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-jsx-a11y": "^6.5.1",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-react": "^7.29.4",
    "eslint-plugin-react-hooks": "^4.4.0",
    "eslint-plugin-security": "^1.4.0",
    "eslint-plugin-simple-import-sort": "^7.0.0",
    "eslint-plugin-sonarjs": "^0.13.0",
    "eslint-plugin-unicorn": "^42.0.0",
    "jest": "^27.5.1",
    "prettier": "^2.6.2",
    "start-server-and-test": "^1.14.0",
    "ts-jest": "^27.1.4",
    "typescript": "^4.6.3"
  }
}
